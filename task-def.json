{
  "containerDefinitions": [
    {
      "volumesFrom": [],
      "memory": 500,
      "portMappings": [
        {
          "hostPort": 0,
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true,
      "entryPoint": [
        "/usr/sbin/apache2",
        "-D",
        "FOREGROUND"
      ],
      "mountPoints": [
        {
          "containerPath": "/var/www/my-vol",
          "sourceVolume": "my-vol"
        }
      ],
      "name": "simple-php-app",
      "environment": [],
      "image": "srikanna/ecs-demo:v_%BUILD_NUMBER%",
      "cpu": 10
    },
    {
      "volumesFrom": [
        {
          "sourceContainer": "simple-php-app"
        }
      ],
      "memory": 500,
      "portMappings": [],
      "essential": false,
      "entryPoint": [
        "sh",
        "-c"
      ],
      "mountPoints": [],
      "name": "busybox",
      "environment": [],
      "image": "busybox",
      "command": [
        "/bin/sh -c \"while true; do /bin/date > /var/www/my-vol/date; sleep 1; done\""
      ],
      "cpu": 10
    }
  ],
  "volumes": [
    {
      "host": {
      },
      "name": "my-vol"
    }
  ],
  "family": "php-sample-app"
}
